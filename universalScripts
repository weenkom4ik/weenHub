local function ESOG_fake_script() -- submit.LocalScript 
	local script = Instance.new('LocalScript', submit)

	script.Parent.Activated:Connect(function()
		if script.Parent.Parent.key.Text == localkey then
			script.Parent.Parent.key.Text = "Success! Loading..."
			wait(1)
			script.Parent.Parent.Visible = false
			wait(1)
			script.Parent.Parent.Parent.ScrollingFrame.Visible = true
		else
			script.Parent.Parent.key.Text = "Incorrect key"
		end
	end)
end
coroutine.wrap(ESOG_fake_script)()
local function MXOMCY_fake_script() -- weenHubUniversal.LocalScript 
	local script = Instance.new('LocalScript', weenHubUniversal)

	game:GetService("StarterGui"):SetCore("SendNotification",{
		Title = "Universal Script Group loaded!",
		Text = "script by weenkom4ik (also creds to sviter)", 
	
		Button1 = "Okay",
		Duration = 30 
	})
end
coroutine.wrap(MXOMCY_fake_script)()
